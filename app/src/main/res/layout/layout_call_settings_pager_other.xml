<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_call_settings_other"
        style="@style/style_call_setting_title"
        android:text="@string/call_setttings_analys"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/v_call_settings_other_1"
        style="@style/style_call_setting_line"
        android:layout_marginTop="10dp"
        app:layout_constrainedWidth="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_call_settings_other" />

    <RelativeLayout
        android:id="@+id/ll_call_settings_other"
        style="@style/style_call_settings_rl"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/v_call_settings_other_1">

        <TextView
            style="@style/style_call_setting_lable"
            android:text="点击右侧按钮上传日志" />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/s_call_settings_other_params"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical|right"
            android:background="@drawable/call_settings_upload_btn"
            android:text="上传"
            android:textColor="@color/colorBlack"
            android:textSize="@dimen/font_normal_big" />

    </RelativeLayout>

    <View
        style="@style/style_call_setting_line"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ll_call_settings_other" />

</android.support.constraint.ConstraintLayout>